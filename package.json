{
  "name": "nagbot",
  "version": "1.0.0",
  "description": "Task nagging leveraging Microsoft Graph and the Bot Framework.",
  "main": "./lib/app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node ./lib/app.js",
    "test:unit": "jest --coverage --detectOpenHandles",
    "test": "npm run lint && npm run test:unit",
    "watch": "concurrently --kill-others \"tsc -w\" \"nodemon --inspect ./lib/app.js\"",
    "lint": "tslint -q -p . -c tslint.json"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "botbuilder": "^4.3.4",
    "botbuilder-ai": "^4.3.4",
    "botframework-config": "^4.3.4",
    "global-tunnel-ng": "^2.7.1",
    "jaeger-tracer-restify": "^1.0.1",
    "mongodb": "^3.2.5",
    "node-fetch": "^2.6.0",
    "prom-client": "^11.3.0",
    "restify": "^8.3.2"
  },
  "devDependencies": {
    "@microsoft/microsoft-graph-types": "^1.8.0",
    "@microsoft/microsoft-graph-types-beta": "github:microsoftgraph/msgraph-typescript-typings#beta",
    "@microsoft/recognizers-text-date-time": "^1.1.4",
    "@types/continuation-local-storage": "^3.2.1",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.13",
    "@types/mongodb": "^3.1.24",
    "@types/node-fetch": "^2.3.4",
    "@types/restify": "^7.2.10",
    "concurrently": "^4.0.1",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
