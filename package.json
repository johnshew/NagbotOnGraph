{
  "name": "nagbot",
  "version": "1.0.0",
  "description": "Task nagging leveraging Microsoft Graph and the Bot Framework.",
  "main": "./lib/app.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node ./lib/app.js",
    "test:unit": "jest --coverage --detectOpenHandles",
    "test": "npm run lint && npm run test:unit",
    "watch": "concurrently --kill-others \"tsc -w\" \"nodemon --inspect ./lib/app.js\"",
    "lint": "tslint -q -p . -c tslint.json",
    "lint:fix": "tslint --fix -p . -c tslint.json"
  },
  "author": "Microsoft",
  "license": "MIT",
  "dependencies": {
    "botbuilder": "^4.5.3",
    "botbuilder-ai": "^4.5.3",
    "botframework-config": "^4.5.3",
    "global-tunnel-ng": "^2.7.1",
    "mongodb": "^3.3.0",
    "node-fetch": "^2.6.0",
    "prom-client": "^11.5.3",
    "qrcode": "^1.4.1",
    "restify": "^8.4.0",
    "ts-jest": "^24.0.2"
  },
  "devDependencies": {
    "@types/qrcode": "^1.3.4",
    "@microsoft/microsoft-graph-types": "^1.10.0",
    "@microsoft/microsoft-graph-types-beta": "github:microsoftgraph/msgraph-typescript-typings#beta",
    "@microsoft/recognizers-text-date-time": "^1.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.17",
    "@types/mongodb": "^3.1.32",
    "@types/node-fetch": "^2.5.0",
    "@types/restify": "^7.2.12",
    "concurrently": "^4.1.1",
    "dotenv": "^8.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">= 10.0.0"
  }
}
